<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:t="urn:import:com.me.testproject.client.ui.tab"
	xmlns:c="urn:import:com.google.gwt.user.cellview.client">

	<g:HTMLPanel>
		<g:HTML>Hello, Good Morning</g:HTML>
		<g:HTML>Here is the list of all users...</g:HTML>
		<c:CellTable ui:field="usersTable"></c:CellTable>
		<g:HTMLPanel>
			<div>
				<g:InlineLabel>Username: </g:InlineLabel>
				<g:TextBox ui:field="userNameBox"></g:TextBox>
			</div>
			<div>
				<g:InlineLabel>Password: </g:InlineLabel>
				<g:TextBox ui:field="passwordBox"></g:TextBox>
			</div>
			<div>
				<g:InlineLabel>City: </g:InlineLabel>
				<g:TextBox ui:field="city"></g:TextBox>
			</div>
			<div>
				<g:InlineLabel>Country: </g:InlineLabel>
				<g:TextBox ui:field="country"></g:TextBox>
			</div>
			<g:Button ui:field="update">Submit</g:Button>
		</g:HTMLPanel>
	</g:HTMLPanel>

</ui:UiBinder> 
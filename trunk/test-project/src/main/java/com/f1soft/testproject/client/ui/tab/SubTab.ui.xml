<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style src='../../resources/defines.css'
		type='com.f1soft.testproject.client.ui.tab.BaseTab.Style'>
		.tab {
		float: left;
		margin-right: 8px;
		padding: 5px 10px 5px 5px;
		font-weight: 11px;
		}

		.active {
		font-weight: normal;

		}

		.inactive {


		}

		.tab a,.tab a:visited, .tab a:hover{
		text-decoration: none;
		}

		.active a , .inactive a:hover{
		color: #4a5e24;
		text-shadow: 0px 1px 0px #adce6d;
		}

		.inactive a {
		font-weight: normal;
		color: #fff;
		text-shadow: 0px 1px 0px #39510b;
		}
	</ui:style>

	<g:Hyperlink addStyleNames="{style.tab} {style.inactive}"
		ui:field="hyperlink" />
</ui:UiBinder> 
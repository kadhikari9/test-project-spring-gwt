<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:with type="com.me.testproject.client.resources.TestProjectResources"
		field="res"></ui:with>

	<ui:style>
		.menu {
		font-weight: bold;
		background: blue;
		}
	</ui:style>


	<g:HTMLPanel>
		<g:HTMLPanel styleName="{res.testProjectCss.tabMainPanel}">
			<g:HTMLPanel>
				<g:HTMLPanel>
					<g:FlowPanel ui:field="tabPanel" />
					<div style="clear: both;" />
				</g:HTMLPanel>
			</g:HTMLPanel>
		</g:HTMLPanel>

		<g:HTMLPanel ui:field="mainContainer">
			<g:FlowPanel ui:field="tabContentContainer" />
		</g:HTMLPanel>
	</g:HTMLPanel>
</ui:UiBinder> 